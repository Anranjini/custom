{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "rangeStrategy": "auto",
  "rangeStrategyOverrides": {
    "full": {
      "matchDepTypes": ["direct"]
    }
  },
  "platform": "github",
  "repositories": [
    "https://github.com/Anranjini/custom.git"
  ],
  "packageRules": [
    {
      "packagePatterns": ["*"],
      "enabled": true
    }
  ]
}
